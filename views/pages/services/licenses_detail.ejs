<!DOCTYPE html>
<html lang="en">

<head>
    <%- include ('../../partials/head') %>
    <link rel="stylesheet" href="/css/partials/list-sidebar.css">
    <link rel="stylesheet" href="/css/pages/education_overview.css">
    <link rel="stylesheet" href="/css/pages/linceses.css">
</head>

<body>
    <header>
        <%- include ('../../partials/header') %>
    </header>
    <main>
        <div class="edu-overview-top text-center">
            <div class="edu-overview-top-content">
                <h1 class="text-white"><%= data.title_cn %></h1>
                <p><%= data.title_en %>
                </p>
                <a href="/contact" class="btn btn-success btn-lg mb-3">CONTACT US</a>
            </div>
        </div>
        <div class="edu-overview-navbar">
            <div class="menu-item <%= data.license_type === '1' ? 'active' : '' %>">
                <a href="/services/vgf/licenses/1"><%= __('区块链牌照申请') %></a>
            </div>
            <div class="menu-item <%= data.license_type === '2' ? 'active' : '' %>">
                <a href="/services/vgf/licenses/2"><%= __('金融牌照申请') %></a>
            </div>
            <div class="menu-item <%= data.license_type === '3' ? 'active' : '' %>">
                <a href="/services/vgf/licenses/3"><%= __('监管查询') %></a>
            </div>
        </div>
        <div class="container wrap-licenses mt-4 mb-4">
            <%- data.content_cn %>
        </div>
    </main>
    <footer>
        <%- include ('../../partials/footer') %>
    </footer>
</body>

</html>